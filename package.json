{
  "name": "chatty_nest_app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "build:android": "eas build --platform android --profile preview",
    "offline": "expo start --offline",
    "publish:expo": "eas update --branch preview && npx sentry-expo-upload-sourcemaps dist",
    "publish:sentry": "bunx sentry-expo-upload-sourcemaps dist",
    "upgrade": "bun install expo@latest && bunx expo install --fix",
    "android": "bunx expo run:android --device",
    "release:android": "bunx expo run:android --device --variant Release",
    "release:ios": "bunx expo run:ios --configuration Release",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.8.2",
    "@clerk/types": "^4.47.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/material-top-tabs": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.3.0",
    "@supabase/supabase-js": "^2.49.1",
    "base64-arraybuffer": "^1.0.2",
    "dayjs": "^1.11.13",
    "expo": "~52.0.37",
    "expo-application": "^6.0.2",
    "expo-auth-session": "^6.0.3",
    "expo-av": "^15.0.2",
    "expo-constants": "~17.0.7",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "^2.0.6",
    "expo-image-picker": "^16.0.6",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "~7.0.5",
    "expo-quick-actions": "^4.0.2",
    "expo-router": "~4.0.17",
    "expo-secure-store": "^14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
